#! /bin/bash

age=25
if [ "$age" -gt 18 ] || [ "$age" -lt 30 ]
then 
   echo "valid age"
   else
      echo "age not valid" 
  fi
ctrl+s save in vc

rake@rake-VirtualBox:~/Desktop$ ./hello.sh
valid age
